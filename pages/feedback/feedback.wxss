page{
  width: 100%;
  height: 100%;
  position: fixed;
}
.contain{
  width: 648rpx;
  min-height: 494rpx;
  background: #ffffff;
  border-radius: 15rpx;
  padding: 40rpx;
position: absolute;
  box-sizing:border-box;
  font-size: 35rpx;
  z-index: 1;
}
.holdcontain{
  width: 648rpx;
  min-height: 494rpx;
  font-size: 25rpx;
  background: #ffffff;
  border-radius: 15rpx;
  padding: 40rpx;
position: absolute;
  box-sizing:border-box;
  font-size: 25rpx;
}

.place-holder{
  font-size: 24rpx;
}
.submit{
  width: 275rpx;
  height: 99rpx;
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  background-repeat: no-repeat;
  background-size: 100% 100%;
  background-image: url(http://wximg.bitaxes.com/miaoEread/feedback/2C1ZKf.png);
}
.hover_submit{
  width: 265rpx;
  height: 80rpx;
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  background-size: 100% 100%;
  background-repeat: no-repeat;
  background-image: url(http://wximg.bitaxes.com/miaoEread/feedback/2Z5McV.png);
}
.outline{
  width: 685rpx;
  min-height:547rpx ;
  margin: 70rpx auto;

  padding: 21rpx;
  box-sizing:border-box;
  position: relative;
}
.outline image{
  position: absolute;
  top: -1rpx;
  width: 688rpx;
  height: 541rpx;
  z-index: 0;
  left: 50%;
  transform: translateX(-50%);
}