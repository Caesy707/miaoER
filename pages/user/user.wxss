    color:#6e6e6e
}

.modal {
    position: relative;
    display: inline-block;
    width: 680rpx;
    max-width: 100%;
    background-color: #f8f8f8;
    border-radius: 10rpx;
    overflow: hidden;
    z-index: 100;
    margin-top: -33%;
    margin-left: 5%;
    opacity: 1;
    transition-duration: 5s;
    overflow-x: hidden;
    overflow-y: auto;
}

.cu-item {
    position: relative;
    display: flex;
    padding: 0 30rpx;
    min-height: 100rpx;
    background-color: var(--white);
    justify-content: space-between;
    align-items: center;
}

.round {
    position: absolute;
    right: 0;
}

.flex-font {
    display: inline;
}

.mask {
    position: fixed;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    background: rgba(0, 0, 0, 0.3);
    outline: 0;
}

